{
  "version": 2,
  "builds": [
    {
      "src": "stats_server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/stats_server.js"
    },
    {
      "src": "/health",
      "dest": "/stats_server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 